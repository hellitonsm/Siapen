    <TfrxDBXQuery Name="sqlsaida" UserName="sqlsaida" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    INTERNO.RGI,&#13;&#10;    interno.mae,&#13;&#10;    interno.pai,&#13;&#10;    interno.endereco,&#13;&#10;    interno.data_nascimento,          &#13;&#10;    unidade_penal.nome_up,&#13;&#10;    unidade_penal.sigla,                                                        &#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    interno.foto,&#13;&#10;    agenda_atendimento.numero_oficio,&#13;&#10;    agenda_atendimento.data_agenda,&#13;&#10;    agenda_atendimento.data_audiencia,&#13;&#10;    agenda_atendimento.hora_audiencia,&#13;&#10;    agenda_atendimento.observacao,&#13;&#10;    destino.responsavel,          &#13;&#10;    destino.destino,&#13;&#10;    destino.funcao        &#13;&#10;FROM INTERNO&#13;&#10;   LEFT JOIN agenda_atendimento on (interno.id_interno=agenda_atendimento.id_interno)&#13;&#10;   LEFT JOIN destino ON (destino.id_destino=agenda_atendimento.iddestino)&#13;&#10;   LEFT join  unidade_penal ON (interno.id_up=unidade_penal.id_up)&#13;&#10;   LEFT JOIN PAVILHAO ON (INTERNO.IDPAVILHAO = PAVILHAO.ID_PAVILHAO)&#13;&#10;   INNER JOIN GALERIA ON (INTERNO.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;WHERE INTERNO.ST = 'A' AND INTERNO.ID_UP = :ID_UP  AND interno.rgi='47896'&#13;&#10;ORDER BY  INTERNO.NOME_INTERNO              " Database="database" PropData="05704C65667403B4000470546F7002640A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
