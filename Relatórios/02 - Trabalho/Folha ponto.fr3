    <TfrxDBXQuery Name="Sqlup" UserName="SqlUp" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)&#13;&#10;WHERE ID_UP = :ID_UP" Database="database" PropData="05704C65667402600470546F7002600A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="SqlTrabalho" UserName="SqlTrabalho" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select &#13;&#10;    cela.cela,&#13;&#10;    pavilhao.pavilhao,&#13;&#10;    galeria.galeria,&#13;&#10;    solario.solario,&#13;&#10;    setor_trabalho.setor_trabalho,&#13;&#10;    historico_trabalho.data_saida,&#13;&#10;    historico_trabalho.motivo_saida,&#13;&#10;    historico_trabalho.data,&#13;&#10;    interno.nome_interno,&#13;&#10;    interno.rgi,&#13;&#10;    historico_trabalho.documento&#13;&#10;from historico_trabalho&#13;&#10;   left join setor_trabalho on (historico_trabalho.id_setor_trabalho = setor_trabalho.id_setor_trabalho)&#13;&#10;   left join interno on (historico_trabalho.id_interno = interno.id_interno)&#13;&#10;   left join cela on (interno.idcela = cela.id_cela)&#13;&#10;   left join pavilhao on (interno.idpavilhao = pavilhao.id_pavilhao)&#13;&#10;   left join galeria on (interno.idgaleria = galeria.id_galeria)&#13;&#10;   left join solario on (interno.idsolario = solario.id_solario)&#13;&#10;&#13;&#10;WHERE Interno.ST = 'A'  AND  Interno.ID_UP = :ID_UP&#13;&#10;  &#13;&#10;AND historico_trabalho.data_saida is null&#13;&#10;  &#13;&#10;AND setor_trabalho.setor_trabalho = :setor_trabalho                                                                                                              &#13;&#10;&#13;&#10;AND ( (interno.nome_interno||' ('||interno.RGI||')' LIKE '%'|| :NOMEINTERNO || '%') )&#13;&#10;    &#13;&#10;ORDER BY Interno.NOME_INTERNO                      " Database="database" PropData=""/>
    <TfrxDBXQuery Name="SqlListaInterno" UserName="SqlListaInterno" CloseDataSource="True" FieldAliases.Text="NOME_INTERNO=NOME_INTERNO&#13;&#10;ID_INTERNO=ID_INTERNO" SQL.Text="select &#13;&#10;    setor_trabalho.setor_trabalho,&#13;&#10;    historico_trabalho.motivo_saida,      &#13;&#10;    interno.nome_interno,&#13;&#10;    interno.rgi      &#13;&#10;from historico_trabalho&#13;&#10;   inner join setor_trabalho on (historico_trabalho.id_setor_trabalho = setor_trabalho.id_setor_trabalho)&#13;&#10;   inner join interno on (historico_trabalho.id_interno = interno.id_interno)&#13;&#10;   &#13;&#10;WHERE Interno.ST = 'A'  AND  Interno.ID_UP = :ID_UP&#13;&#10;AND historico_trabalho.data_saida is null&#13;&#10;AND coalesce (interno.nome_interno,'')&#60;&#62;''   &#13;&#10;&#13;&#10;ORDER BY Interno.NOME_INTERNO  &#13;&#10;  " Database="database" PropData="05704C65667403C4000470546F7002600A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
