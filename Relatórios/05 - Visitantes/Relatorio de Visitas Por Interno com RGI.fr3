    <TfrxDBXQuery Name="sqlvisitante" UserName="sqlvisitante" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" DataSetOptions="0" IgnoreDupParams="False" SQL.Text="select v.visitante,&#13;&#10;i.nome_interno, g.grau_parentesco, mvd.data_visita                                   &#13;&#10;from mov_diario_visitantes mvd            &#13;&#10;left join visitante v on (mvd.id_visitante = v.id_visitante)&#13;&#10;left join interno i on (mvd.id_interno = i.id_interno)&#13;&#10;left join cela c on (i.idcela = c.id_cela)&#13;&#10;inner join visitante_interno vi&#13;&#10;on vi.id_visitante = v.id_visitante and&#13;&#10;   vi.id_interno = i.id_interno&#13;&#10;left join grau_parentesco g on (vi.id_grau_parentesco = g.id_grau_parentesco)&#13;&#10;       &#13;&#10;where mvd.DATA_VISITA between :data and :data1&#13;&#10;and mvd.id_up = :id_up&#13;&#10;and i.rgi = :nome_interno                                      &#13;&#10;order by i.nome_interno                                                       " Database="Database" PropData=""/>
