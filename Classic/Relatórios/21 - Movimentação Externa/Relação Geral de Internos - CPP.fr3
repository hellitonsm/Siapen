    <TfrxDBXQuery Name="Sqltrabalho" UserName="Sqltrabalho" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT                    &#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    INTERNO.RGI,&#13;&#10;    interno.data_setor,          &#13;&#10;    unidade_penal.nome_up,&#13;&#10;    unidade_penal.sigla,                                                        &#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    interno.doc_trabalho,&#13;&#10;    interno.data_setor,  &#13;&#10;    SOLARIO.SOLARIO, &#13;&#10;    LOCAL_TRABALHO.local_trabalho,&#13;&#10;    INTERNO.regime_atual,&#13;&#10;    TRABALHO.observacao_trabalho,&#13;&#10;    BENEFICIO.beneficio,&#13;&#10;    IIF(COALESCE( local_trabalho.local_trabalho, '') = '', 'NÃO TRABALHA', local_trabalho.local_trabalho) AS OCUPACAO      &#13;&#10;           &#13;&#10;      &#13;&#10;FROM INTERNO&#13;&#10;   LEFT join  unidade_penal ON (interno.id_up=unidade_penal.id_up)&#13;&#10;   LEFT JOIN BENEFICIO ON (INTERNO.id_interno = BENEFICIO.id_interno)&#13;&#10;   LEFT JOIN TRABALHO ON (INTERNO.id_interno = TRABALHO.id_interno)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;   left JOIN local_trabalho ON (INTERNO.id_local_trabalho = local_trabalho.id_local_trabalho)&#13;&#10;WHERE INTERNO.ST = 'A'&#13;&#10;AND INTERNO.ID_UP = :ID_UP &#13;&#10;ORDER BY INTERNO.NOME_INTERNO" Database="Database" PropData="05704C65667403AC000470546F70025C0A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
