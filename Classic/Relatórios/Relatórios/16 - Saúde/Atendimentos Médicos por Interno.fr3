    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="Database" PropData="05704C65667403D4000470546F7002200A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="SqlAtendimentoMedico" UserName="SqlAtendimentoMedico" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select i.rgi, i.nome_interno, g.galeria, s.solario, c.cela, h.descricao, h.data&#13;&#10;from historico_clinica_medica h&#13;&#10;inner join interno i on (h.id_interno = i.id_interno)  &#13;&#10;inner join pavilhao p on (i.idpavilhao = p.id_pavilhao)  &#13;&#10;inner join galeria g on (i.idgaleria = g.id_galeria)  &#13;&#10;inner join cela c on (i.idcela = c.id_cela)  &#13;&#10;inner join solario s on (i.idsolario = s.id_solario)  &#13;&#10;where (( (i.nome_interno||' ('||i.RGI||')' LIKE '%'|| :NOMEINTERNO || '%' ) )&#13;&#10;or (i.rgi= :rgi))    &#13;&#10;order by h.data&#13;&#10;  " Database="Database" PropData=""/>
    <TfrxDBXQuery Name="SqlTotal" UserName="SqlTotal" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select count(id_historico_clinica_medica) as TOTAL                                                          &#13;&#10;from historico_clinica_medica h&#13;&#10;inner join interno i on (h.id_interno = i.id_interno)  &#13;&#10;inner join pavilhao p on (i.idpavilhao = p.id_pavilhao)  &#13;&#10;inner join galeria g on (i.idgaleria = g.id_galeria)  &#13;&#10;inner join cela c on (i.idcela = c.id_cela)  &#13;&#10;inner join solario s on (i.idsolario = s.id_solario)  &#13;&#10;where (( (i.nome_interno||' ('||i.RGI||')' LIKE '%'|| :NOMEINTERNO || '%' ) )&#13;&#10;or (i.rgi= :rgi))    &#13;&#10;  " Database="Database" PropData=""/>
