    <TfrxDBXQuery Name="Sqltrabalho" UserName="Sqltrabalho" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    INTERNO.RGI,&#13;&#10;    interno.data_setor,          &#13;&#10;    unidade_penal.nome_up,&#13;&#10;    unidade_penal.sigla,                                                        &#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    setor_trabalho.setor_trabalho,&#13;&#10;    interno.doc_trabalho,&#13;&#10;    local_trabalho.local_trabalho,&#13;&#10;    interno.data_setor&#13;&#10;FROM INTERNO&#13;&#10;&#13;&#10;   LEFT JOIN setor_trabalho ON (setor_trabalho.id_setor_trabalho=interno.idsetor_trabalho)&#13;&#10;   LEFT join  unidade_penal ON (interno.id_up=unidade_penal.id_up)&#13;&#10;   LEFT JOIN PAVILHAO ON (INTERNO.IDPAVILHAO = PAVILHAO.ID_PAVILHAO)&#13;&#10;   INNER JOIN GALERIA ON (INTERNO.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;   left JOIN local_trabalho ON (INTERNO.id_local_trabalho = local_trabalho.id_local_trabalho)&#13;&#10;WHERE INTERNO.ST = 'A' AND INTERNO.ID_UP = :ID_UP AND SOLARIO.SOLARIO=:SOLARIO                                                            &#13;&#10;ORDER BY  SOLARIO.SOLARIO, NOME_INTERNO                                                                     " Database="Database" PropData=""/>
