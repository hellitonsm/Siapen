    <TfrxDBXQuery Name="sqlconfere" UserName="sqlconfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select &#13;&#10;     coalesce(galeria.galeria, '') as galeria,  &#13;&#10;     coalesce(solario.solario, '') as solario,  &#13;&#10;     coalesce(cela.cela, '') as cela,  &#13;&#10;     coalesce(interno.nome_interno, '') as nome_interno,  &#13;&#10;     coalesce(interno.rgi, '') as rgi,  &#13;&#10;     coalesce(interno.data_entrada, '') as data_entrada                                    &#13;&#10;from interno&#13;&#10;   inner join galeria on (interno.idgaleria = galeria.id_galeria)&#13;&#10;   inner join solario on (interno.idsolario = solario.id_solario)&#13;&#10;   inner join cela on (interno.idcela = cela.id_cela)&#13;&#10;   inner join unidade_penal u on (interno.id_up = u.id_up)                                                                          &#13;&#10;                                                                      &#13;&#10;where INTERNO.ST = 'A' AND INTERNO.ID_UP = :ID_UP&#13;&#10;AND u.sigla || ' - Bloco ' || galeria.galeria LIKE '%' || :GALERIA || '%'&#13;&#10;order by galeria.galeria, solario.solario, cela.cela,  interno.nome_interno, interno.data_entrada  " Database="database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;left join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="database" PropData="05704C6566740314010470546F7002240A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="SqlListagem" UserName="SqlListagem" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select &#13;&#10;    unidade_penal.sigla || ' - Bloco ' || galeria.galeria as nome&#13;&#10;from unidade_penal                                              &#13;&#10;   inner join pavilhao on (unidade_penal.id_up = pavilhao.id_up)&#13;&#10;   inner join galeria on (pavilhao.id_pavilhao = galeria.idpavilhao)&#13;&#10;where &#13;&#10;   pavilhao.id_up = :id_up  &#13;&#10;&#13;&#10;ORDER BY galeria&#13;&#10;&#13;&#10;    " Database="database" PropData="05704C6566740310010470546F7002700A506172616D657465727301010C3F000000204E616D653D2269645F7570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
