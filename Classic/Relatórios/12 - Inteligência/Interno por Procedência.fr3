    <TfrxDBXQuery Name="Sqlendereco" UserName="Sqlendereco" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select i.rgi, i.nome_interno, i.endereco, i.fone, p.pavilhao, g.galeria,&#13;&#10;c.cela, s.solario, n.nome_up, pr.proedencia                                      &#13;&#10;&#13;&#10;&#13;&#10;from interno i&#13;&#10;&#13;&#10;&#13;&#10;   INNER JOIN PAVILHAO p ON (I.idpavilhao = P.id_pavilhao)&#13;&#10;   INNER JOIN GALERIA g ON (I.idgaleria = G.id_galeria)&#13;&#10;   INNER JOIN CELA c ON (I.idcela = C.id_cela)&#13;&#10;   INNER JOIN SOLARIO s ON (I.idsolario = S.id_solario)&#13;&#10;   INNER JOIN unidade_penal n ON (I.id_up = n.id_up)&#13;&#10;   LEFT JOIN procedencia pr ON(i.id_procedencia = pr.id_procedencia)                                                                                                                                                         &#13;&#10;&#13;&#10;WHERE I.ST = 'A' AND I.ID_UP = :ID_UP AND pr.proedencia  like '%' || :procedencia || '%'                    &#13;&#10;order by pr.proedencia                                                                                           " Database="database" PropData=""/>
