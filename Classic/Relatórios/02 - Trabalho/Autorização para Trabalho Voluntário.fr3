    <TfrxDBXQuery Name="sqlinterno" UserName="sqlinterno" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;    i.rgi,&#13;&#10;    i.pai,&#13;&#10;    i.mae,&#13;&#10;    i.endereco,&#13;&#10;    i.bairro,&#13;&#10;    i.regime_atual,&#13;&#10;    i.numero,&#13;&#10;    i.complemeneto, &#13;&#10;    i.nome_interno,&#13;&#10;    G.GALERIA,&#13;&#10;    S.SOLARIO,&#13;&#10;    C.CELA,&#13;&#10;    p.pavilhao,&#13;&#10;    up.sigla,&#13;&#10;    st.setor_trabalho,&#13;&#10;    fi.funcao_interno&#13;&#10;&#13;&#10;&#13;&#10;FROM interno I&#13;&#10;INNER JOIN GALERIA g ON (I.IDGALERIA = G.ID_GALERIA)&#13;&#10;INNER JOIN CELA c ON (i.IDCELA = C.ID_CELA)&#13;&#10;INNER JOIN SOLARIO s ON (I.IDSOLARIO = S.ID_SOLARIO)&#13;&#10;inner join pavilhao p on (i.idpavilhao = p.id_pavilhao)&#13;&#10;INNER JOIN UNIDADE_PENAL up ON (p.ID_UP = up.ID_UP)&#13;&#10;left join setor_trabalho st on (st.id_setor_trabalho = i.idsetor_trabalho)&#13;&#10;left join funcao_interno fi on (fi.id_funcao_interno = i.id_funcaointerno)&#13;&#10;where  I.ID_UP = :ID_UP&#13;&#10;AND ( (i.NOME_INTERNO||' ('||i.RGI||')'||' ID:'||i.id_interno = :nome_interno)&#13;&#10;or (i.RGI = :RGI)  &#13;&#10;)                                        " Database="Database" PropData=""/>
